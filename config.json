{
  "gulp": {
    "src": "./src",
    "build": "./build",
    "dist": "./dist",
    "pug": "./src/*.pug",
    "less": {
      "src": "./src/css/**/*.less",
      "out": "app.css"
    },
    "js": {
      "watch": "./src/js/**/*.js",
      "src": "./src/js/index.js",
      "out": "app.js"
    }
  },
  "cdn": {
    "js": {
      "jquery": "//ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js",
      "angular": "//ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular.min.js",
      "angular-animate": "//ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular-animate.min.js",
      "angular-aria": "//ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular-aria.min.js",
      "angular-messages": "//ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular-messages.min.js",
      "angular-material": "//ajax.googleapis.com/ajax/libs/angular_material/1.0.7/angular-material.min.js"
    },
    "css": {
      "angular-material": "//ajax.googleapis.com/ajax/libs/angular_material/1.0.7/angular-material.min.css"
    }
  }
}
