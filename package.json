{
  "name": "lisk-nano",
  "version": "0.0.1",
  "description": "Lisk Nano",
  "homepage": "https://github.com/LiskHQ/lisk-nano",
  "bugs": "https://github.com/LiskHQ/lisk-nano/issues",
  "scripts": {
    "clean": "rimraf build",
    "index": "pug -P -o build src/index.pug",
    "build": "npm run clean && npm run index && webpack --progress --profile -p",
    "server": "npm run clean && npm run index && webpack-dev-server --history-api-fallback --inline --progress --content-base build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LiskHQ/lisk-nano"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "angular-animate": "^1.5.6",
    "angular-aria": "^1.5.6",
    "angular-material": "^1.1.0-rc4-master-06e7e99",
    "angular-messages": "^1.5.6",
    "babel-polyfill": "^6.9.1",
    "jquery": "^2.2.4"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "pug": "^2.0.0-alpha8",
    "pug-cli": "^1.0.0-alpha5",
    "pug-html-loader": "^1.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "license": "MIT"
}
