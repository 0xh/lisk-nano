md-dialog.login-dialog(aria-label='Lisk Nano')
  md-toolbar
    .md-toolbar-tools
      h2 Lisk Nano
  md-dialog-content
    md-tabs(md-dynamic-height)
      md-tab(label='SIGN IN')
        md-content.md-padding
          md-input-container.md-block
            label Passphrase
            input(ng-model='passphrase')
        div.md-padding(layout="row", layout-align="center end")
          md-button.md-raised.md-primary(ng-disabled='passphrase.valid') Login
      md-tab(label='NEW ACCOUNT')
        md-content.md-padding
